/* Reset */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td{margin:0;padding:0;}table{border-collapse:collapse;border-spacing:0;margin:0;padding:0;}fieldset,img{display:block; border:0;}address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal;}ol,ul {list-style:none;}caption,th {text-align:left;}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}q:before,q:after{content:'';}abbr,acronym {border:0;}

body, input, textarea{
	font-family: 'Roboto', sans-serif;
}
*{
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	box-sizing:border-box;
}
*::after{
	content:'';
	display:block;
	clear:both;
}
a::after{
	display:none;
}
a{
	color:#808285;
	text-decoration:none;
}
.wrapper{
	width:100%;
	overflow:hidden;
}
header{
	width:100%;
	height:155px;
	background:#fff;
	border-bottom:1px solid #808285;
}
.headerBox{
	width:960px;
	margin:0 auto;
	padding:65px 0 0;
	position:relative;
}
#logo{
	width:253px;
	height:86px;
	display:block;
	background:url(../images/logo.png) no-repeat top center;
	text-indent:-9999px;
}
nav{
	position:absolute;
	bottom:15px;
	right:0;
}
nav ul li{
	float:left;
	margin-left:25px;
	font-size:14px;
}
nav ul li a{
	padding-bottom:5px;
}
nav ul li a:hover,
nav ul li a.selected{
	color:#1C75BC;
	font-weight:700;
	border-bottom:1px solid #1C75BC;
}
.loginBox{
	position:absolute;
	top:20px;
	right:0;
}
.loginBox ul li{
	float:left;
	font-size:12px;
	border-right:1px solid #808285;
	padding-right:5px;
	margin-right:5px;
}
.loginBox ul li:last-child{
	border:none;
	margin:0;
	padding:0;
}
.headerBox-mobile {
	display: none;
}
.sliderWrapper{
	padding:60px 0 0;
	background:url(../images/blue_tribal_bg.jpg) no-repeat top center;
	overflow:hidden;
	margin:0 0 50px;
}
.sliderBox{
	width:960px;
	margin:0 auto;
}
.sliderInfoBox{
	width:340px;
	overflow:hidden;
	float:left;
	font-size:28px;
	color:#fff;
	font-weight:700;
}
.loanWrapper{
	width:580px;
	padding:45px 25px 20px;
	background: #FFF; /* For browsers that do not support gradients */
    background: -webkit-linear-gradient(#FFF, #F1F2F2); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#FFF, #F1F2F2); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#FFF, #F1F2F2); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#FFF, #F1F2F2); /* Standard syntax */
	float:right;
	border-radius:5px;
}
.loanBox{
	width:290px;
	float:left;
}
.loanBox h3,
