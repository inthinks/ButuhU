/*Step Loan*/
.stepWrapper{
	width:960px;
	text-align:center;
	margin:30px auto 60px;
}
.stepWrapper ul{
	overflow:hidden;
	position:relative;
}
.stepWrapper ul:before{
	content:'';
	width:360px;
	display:block;
	border-top:2px dashed #0B5689;
	position:absolute;
	top:30px;
	left:50%;
	-webkit-transform:translateX(-50%);
	-moz-transform:translateX(-50%);
	transform:translateX(-50%);
	z-index:-1;
}
.stepWrapper ul li{
	width:70px;
	display:inline-block;
	overflow:hidden;
	margin:0 25px;
}
.stepWrapper .stepImage{
	width:60px;
	height:60px;
	display:block;
	border-radius:60px;
	border:1px solid #1C75BC;
	background:#fff;
	line-height:60px;
	font-size:16px;
	font-weight:700;
	color:#1C75BC;
	margin:0 auto 5px;
}
.stepWrapper ul li.selected .stepImage{
	background:#27AAE1;
	border:1px solid #27AAE1;
	color:#fff;
}
.stepWrapper ul li.orange .stepImage{
	background:#F5B419;
	border:1px solid #F5B419;
	color:#58595B;
}
.stepWrapper ul li.done .stepImage{
	background:#1C75BC;
	color:#fff;
}
.stepWrapper .stepInfo{
	height:34px;
	color:#1C75BC;
	font-size:14px;
	font-weight:700;
	overflow:hidden;
}
.stepWrapper ul li.selected .stepInfo{
	color:#27AAE1;
}
.stepWrapper ul li.orange .stepInfo{
	color:#F5B419
}
